		"rss-parser": "^3.13.0",
		"dotenv": "^16.3.1"
	},
	"devDependencies": {
		"eslint": "^8.55.0",
		"prettier": "^3.1.0",
		"nodemon": "^3.0.2"
	},
	"engines": {
		"node": ">=18.0.0"
