{
	"name": "infobot-discord",
	"version": "1.0.0",
	"description": "A Discord bot that automatically monitors social media accounts and sends notifications",
	"main": "src/index.js",
	"type": "module",
	"scripts": {
		"start": "node src/index.js",
		"dev": "node --watch src/index.js",
		"test": "node src/utils/test.js",
		"status": "node src/utils/status.js",
		"config": "node src/config/config.js",
		"lint": "eslint src/",
		"format": "prettier --write src/"
	},
	"keywords": [
